{
  "name": "pagopa-api-config-fe",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "prestart": "export $(grep -v '^#' .env.local | xargs) && chmod +x env.sh && source env.sh",
    "start:local": "NODE_ENV=local parcel --port 3000 src/index.html --dist-dir build/debug",
    "start:DEV": "NODE_ENV=dev parcel --port 3000 src/index.html --dist-dir build/debug",
    "start:UAT": "NODE_ENV=uat parcel --port 3000 src/index.html --dist-dir build/debug",
    "start:PROD": "NODE_ENV=prod parcel --port 3000 src/index.html --dist-dir build/debug",
    "prebuild": "npm-run-all prebuild:*",
    "prebuild:clean": "shx rm -rf build",
    "prebuild:tsc": "tsc --noEmit",
    "build": "parcel build src/index.html --dist-dir build/release --public-url ./ --no-optimize",
    "test": "jest --coverage --ci --testResultsProcessor jest-sonar-reporter",
    "test_old": "jest --coverage --ci",
    "lint": "eslint -f json -o eslint-report.json . -c .eslintrc.js --ext .ts,.tsx --cache",
    "lint:local": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "generate:api": "rm -f swagger.json && wget https://raw.githubusercontent.com/pagopa/pagopa-api-config/main/openapi/swagger.json && sed -i 's/\\\\/\\\\\\\\/g' swagger.json && gen-api-models --api-spec swagger.json --no-strict --out-dir generated/api --request-types --response-decoders --client",
    "macosx:generate:api": "rm -f swagger.json && wget https://raw.githubusercontent.com/pagopa/pagopa-api-config/main/openapi/swagger.json && sed -i '' 's/\\\\/\\\\\\\\/g' swagger.json && gen-api-models --api-spec swagger.json --no-strict --out-dir generated/api --request-types --response-decoders --client",
    "generate": "npm-run-all generate:*",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\""
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "generated": "^0.0.3",
    "i18next": "^19.8.4",
    "react": "17.0.1",
    "react-bootstrap": "^1.6.4",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.7.3",
    "react-icons": "^4.3.1",
    "react-router-dom": "^5.3.0",
    "react-test-renderer": "^17.0.2",
    "react-toastify": "^8.0.3",
    "typeface-lora": "^1.1.13",
    "typeface-roboto-mono": "^1.1.13",
    "typeface-titillium-web": "^1.1.13",
    "fp-ts": "^2.11.3",
    "io-ts": "^2.2.13",    
    "@pagopa/ts-commons": "^10.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@pagopa/eslint-config": "^1.1.1",
    "@pagopa/openapi-codegen-ts": "^10.0.3",
    "@parcel/transformer-sass": "^2.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.9",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-jest": "^27.2.5",
    "bootstrap-italia": "^1.4.3",
    "cssnano": "^5.0.8",
    "danger-plugin-digitalcitizenship": "^1.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "italia-ts-commons": "^8.6.0",
    "jest": "^27.0.6",
    "jest-sonar-reporter": "^2.0.0",
    "jquery": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0",
    "prettier": "2.1.0",
    "sass": "^1.42.1",
    "shx": "^0.3.3",
    "ts-jest": "^27.0.6",
    "typescript": "^4.1.2",
    "whatwg-fetch": "^3.6.2"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  }
}
